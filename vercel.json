{
  "version": 2,
  "builds": [
    {
      "src": "blitz-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/rpc/(.*)",
      "dest": "blitz-app/api/rpc/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "blitz-app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blitz-app/$1"
    }
  ],
  "buildCommand": "cd blitz-app && npm run build",
  "cache": false
}

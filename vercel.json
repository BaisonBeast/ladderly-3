{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "blitz-app/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/blitz/(.*)",
      "dest": "/blitz-app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

{
  "name": "ladderly-3",
  "version": "2.0.0",
  "scripts": {
    "backup": "npm run checklists:backup && npm run users:backup",
    "db:clean": "npm run backup && node scripts/deleteUnusedChecklists.js && node scripts/deleteUnusedChecklistItems.js",
    "dev": "blitz dev",
    "build": "node scripts/generate-sitemap.js && blitz prisma generate && blitz build",
    "migrate": "npm run users:backup && blitz prisma migrate dev && npm run users:restore",
    "seed": "blitz db seed",
    "seed:update-in-place": "blitz db seed -- --update-latest-checklists",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepare": "husky install",
    "checklists:backup": "node scripts/backupChecklists.js",
    "users:backup": "node scripts/backupUsers.js",
    "users:restore": "node scripts/restoreUsers.js"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 80
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.8",
    "@blitzjs/next": "2.0.8",
    "@blitzjs/rpc": "2.0.8",
    "@prisma/client": "5.4.2",
    "@tailwindcss/typography": "0.5.13",
    "@vercel/analytics": "1.2.2",
    "blitz": "2.0.8",
    "final-form": "4.20.10",
    "formik": "2.4.6",
    "gray-matter": "4.0.3",
    "next": "14.1.4",
    "nextjs-google-analytics": "2.3.3",
    "postmark": "4.0.2",
    "prisma": "5.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-select": "5.8.0",
    "secure-password": "4.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.3",
    "@next/env": "13.4.19",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "18.11.9",
    "@types/preview-email": "2.0.1",
    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@vitejs/plugin-react": "2.2.0",
    "ajv": "^7.0.0",
    "autoprefixer": "10.4.19",
    "dotenv": "16.4.5",
    "eslint": "8.27.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-tailwindcss": "3.15.1",
    "fs-extra": "11.2.0",
    "glob": "10.3.12",
    "husky": "8.0.2",
    "jsdom": "20.0.3",
    "lint-staged": "13.0.3",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-prisma": "4.4.0",
    "prettier-plugin-tailwindcss": "0.5.14",
    "pretty-quick": "4.0.0",
    "preview-email": "3.0.7",
    "rehype-external-links": "3.0.0",
    "rehype-stringify": "10.0.0",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "stylelint": "16.5.0",
    "stylelint-config-standard": "36.0.0",
    "tailwindcss": "3.4.3",
    "typescript": "^4.8.4",
    "unified": "11.0.4",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "0.25.3",
    "xml-beautifier": "0.5.0"
  }
}
